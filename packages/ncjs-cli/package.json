{
  "name": "ncjs-cli",
  "description": "NodeCore CLI",
  "version": "1.2.1",
  "author": "VeriBlock",
  "bin": {
    "ncjs-cli": "./bin/run"
  },
  "bugs": "https://github.com/VeriBlock/nodecore-js/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@veriblock/nodecore-parser": "^1.1.10",
    "bech32-buffer": "^0.1.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "eslint": "^6.8",
    "eslint-config-oclif": "^3.1",
    "globby": "^11",
    "prettify": "^0.1.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/VeriBlock/nodecore-js",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "ncjs-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "VeriBlock/nodecore-js",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "gitHead": "14ebeb8ca38664eec3ce2a2282ad82f5e12526ba"
}
